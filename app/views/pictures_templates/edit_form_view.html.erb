<h1>Edit photo data</h1>
<form action="/update_photo/<%= @edit_photo.id %>">
  <div>
    <label for="source_input">
      Image URL
    </label>

    <input id="source_input" type="text" name="the_source" value="<%= @edit_photo.source %>">
  </div>

  <div>
    <label for="caption_input">
      Caption
    </label>

    <input id="caption_input" type="text" name="the_caption" value="<%= @edit_photo.caption %>">
  </div>

  <button>
    Update Photo
  </button>
</form>
